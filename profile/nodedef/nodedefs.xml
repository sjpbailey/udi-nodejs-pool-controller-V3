<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller, this must mach the id in your Controller class-->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
            <st id="GV0" editor="bool" />
            <st id="GV1" editor="TEMP" />
            <st id="GV2" editor="TEMP" />
            <st id="GV3" editor="TEMP" />
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends>
                <!-- These are for our heartbeat -->
                <cmd id="DON" />
                <cmd id="DOF" />
            </sends>
            <accepts>
                <cmd id="QUERY" />
            </accepts>
        </cmds>
    </nodeDef>

    <!-- bodydeid -->
    <nodeDef id="poolnode" nls="bodypool">
        <editors />
        <sts>
            <st id="GV0" editor="bool" />
            <st id="GV1" editor="TEMP" />
            <st id="GV2" editor="TEMP" />
            <st id="GV3" editor="TEMP" />
            <st id="ST" editor="bool" />

        </sts>
        <cmds>
            <sends />
            <accepts>
                <!--cmd id="DON" />
                <cmd id="DOF" /-->
                <cmd id="QUERY" />
            </accepts>
        </cmds>
    </nodeDef>

    <!-- switchnodeid -->
    <nodeDef id="switchnodeid" nls="switch0">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
            <!--st id="GV1" editor="bool" /-->
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="QUERY" />
            </accepts>
        </cmds>
    </nodeDef>

</nodeDefs>
